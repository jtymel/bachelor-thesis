<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:thesui="urn:import:org.jboss.ci.tracker.client.widgets"
>
    <ui:style src="style.css" />
    <g:RootLayoutPanel>
        <g:layer left="0px" right="0px" top="0px" bottom="0px">
            <g:DockLayoutPanel unit="EM">
                <g:north size="2.0">
                    <g:MenuBar>
                        <g:MenuItem>Tools<g:MenuBar vertical="true">
                                <g:MenuItem ui:field="menuParameterizations">Set parameterizations</g:MenuItem>
                            </g:MenuBar>
                        </g:MenuItem>
                    </g:MenuBar>
                </g:north>

                <g:center>
                    <thesui:MainPanel ui:field="mainPanel" />
                </g:center>
            </g:DockLayoutPanel>
        </g:layer>
    </g:RootLayoutPanel>
</ui:UiBinder>