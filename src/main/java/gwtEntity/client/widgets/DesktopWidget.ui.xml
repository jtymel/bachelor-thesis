<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:thesui="urn:import:gwtEntity.client.widgets"
    >
    <ui:style>

    </ui:style>
    <g:RootLayoutPanel>
        <g:layer left="0px" right="0px" top="0px" bottom="0px">
            <g:DockLayoutPanel unit="EM">
                <g:north size="2.0">
                    <g:MenuBar>
                        <g:MenuItem>Tools<g:MenuBar vertical="true">
                            <g:MenuItem ui:field="menuDownloadAll">Download all results of selected job</g:MenuItem>
                        </g:MenuBar></g:MenuItem>
                    </g:MenuBar>
                </g:north>

                <g:center>
                    <thesui:MainPanel ui:field="mainPanel" />
